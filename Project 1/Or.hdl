/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a , out=temp1 );
    Not(in=b , out=temp2 );
    Nand(a=temp1 , b=temp2 , out=out );
    //// Replace this comment with your code.
}